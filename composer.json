{
  "name": "yarcode/yii2-queue-mailer",
  "description": "Queue mailer decorator for Yii2 framework",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "async",
    "mailer",
    "queue"
  ],
  "homepage": "http://packagist.org/packages/yarcode/yii2-queue-mailer",
  "license": "MIT",
  "authors": [
    {
      "name": "Alexey Samoylov",
      "email": "alexey.samoylov@gmail.com",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/yarcode/yii2-queue-mailer/issues",
    "source": "https://github.com/yarcode/yii2-queue-mailer"
  },
  "require": {
    "php": ">=5.6",
    "yiisoft/yii2": "~2",
    "yiisoft/yii2-queue": "~2"
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "yiisoft/yii2-swiftmailer": "~2"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-win": "vendor\\bin\\phpunit.bat"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "YarCode\\Yii2\\QueueMailer\\": "./src"
    }
  }
}
